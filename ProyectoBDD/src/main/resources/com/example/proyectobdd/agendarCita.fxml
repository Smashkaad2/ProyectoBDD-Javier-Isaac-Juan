<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<AnchorPane prefHeight="189.0" prefWidth="251.0"
            xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.proyectobdd.controller.ControllerAgendarCita">
    <children>
        <Label layoutX="14.0" layoutY="14.0" text="Agendar Cita">
            <font>
                <Font size="35.0" />
            </font>
        </Label>
        <DatePicker fx:id="datePickerFecha" layoutX="115.0" layoutY="79.0" prefHeight="25.0" prefWidth="112.0" />
        <ComboBox fx:id="comboBoxHora" layoutX="115.0" layoutY="109.0" prefHeight="25.0" prefWidth="112.0" />
        <Label layoutX="21.0" layoutY="82.0" text="Fecha" />
        <Label layoutX="21.0" layoutY="113.0" text="Hora" />
        <Button layoutX="21.0" layoutY="150.0" mnemonicParsing="false" onAction="#onClickAgendar" text="Agendar" />
    </children>
</AnchorPane>
